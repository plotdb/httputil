{
  "author": "zbryikt",
  "name": "@plotdb/httputil",
  "license": "MIT",
  "description": "util functions for http",
  "version": "0.0.5",
  "browser": "dist/index.min.js",
  "main": "dist/index.min.js",
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/plotdb/httputil",
  "repository": {
    "type": "git",
    "url": "https://github.com/plotdb/httputil"
  },
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "start": "npx server -r web",
    "test": "npx mocha --require livescript ./test/index.ls",
    "coverage": "npx nyc --reporter=text npm run test"
  },
  "devDependencies": {
    "@loadingio/debounce.js": "^1.0.1",
    "@loadingio/ldquery": "^3.0.4",
    "bootstrap": "^4.5.2",
    "@loadingio/bootstrap.ext": "^0.0.9",
    "bootstrap.native": "^3.0.13",
    "fedep": "^1.1.7",
    "ldcover": "^3.3.0",
    "ldiconfont": "^0.2.3",
    "ldview": "^1.3.0",
    "livescript": ">=1.6.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "@zbryikt/template": "^2.3.41",
    "uglify-js": "^3.13.1",
    "browserify": "^17.0.0"
  },
  "nyc": {
    "all": true,
    "extension": ".ls",
    "require": [
      "livescript"
    ],
    "include": [
      "src/**/*"
    ]
  },
  "frontendDependencies": {
    "root": "web/static/assets/lib",
    "modules": [
      "bootstrap",
      "bootstrap.native",
      "@loadingio/bootstrap.ext",
      "@loadingio/ldquery",
      "@loadingio/debounce.js",
      "ldview",
      "ldcover",
      "ldiconfont"
    ]
  }
}
